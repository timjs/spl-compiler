Int matcher (Int i ,Bool b)
{
	(Int,Bool) pair = (i,b);

    match pair
    case ((0,True))
        return 0;
    case ((0,False))
    	return -1;
    case ((1,True))
    	return 1;
    case ((1,False))
    	return 2;
}

Void main () {
	print(matcher(0,True)); // => 0
	print(matcher(1,False));// => 2
	print(matcher(2,False));// => ?
}

