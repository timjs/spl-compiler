Int matcher (Int integer)
{
    Int x = 0;

    match integer
    case (0)
        return 0;
    case (1)
        return 2;
    case (2)
    	return 4;
    case (4)
    	return 8;
    case (x)
    	return x*3;
}

Void main () {
	print(matcher(0));// => 0
	print(matcher(1));// => 2
	print(matcher(2));// => 4
	print(matcher(3));// => 9
	print(matcher(4));// => 8
	print(matcher(5));// => 15
}

